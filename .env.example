# Database
DATABASE_URL=postgres://user:password@localhost:5432/boxbox?sslmode=disable

# Daytona
DAYTONA_API_KEY=your-daytona-api-key
DAYTONA_API_URL=https://api.daytona.io
DAYTONA_IMAGE=daytonaio/ai-python-3.12:latest

# S3 Storage
S3_BUCKET=your-bucket-name
S3_REGION=us-east-1
S3_ACCESS_KEY_ID=your-access-key
S3_SECRET_ACCESS_KEY=your-secret-key
S3_ENDPOINT=http://localhost:9000

# Pool Settings
POOL_MIN_WARM=5
POOL_MAX_WARM=50
POOL_SCALE_UP_THRESHOLD=2
POOL_SCALE_DOWN_THRESHOLD=20
POOL_SANDBOX_TTL=60m

# API
API_PORT=8080
API_KEY=your-secure-api-key

# Execution
EXECUTION_TIMEOUT=1m
MAX_FILE_SIZE_MB=100
SYNC_CONCURRENCY=5
